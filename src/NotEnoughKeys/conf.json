{
  "hook": {
    "CapsLock & h": "left",
    "CapsLock & j": "down",
    "CapsLock & k": "up",
    "CapsLock & l": "right",
    "CapsLock & space": "enter",
    "CapsLock & 4": "end",
    "CapsLock & 6": "home",
    "CapsLock & d": "delete",
    "CapsLock & q": {
      "special": "MoveWindow"
    },
    "CapsLock & e": {
      "special": "ResizeWindow"
    },
    "CapsLock & u": {
      "raw": "hmm",
      "when": {
        "exe": "notepad.exe"
      }
    }
  },
  "hotkey": {
    "Ctrl & Alt & t": {
      "run": "wt"
    }
  }
}